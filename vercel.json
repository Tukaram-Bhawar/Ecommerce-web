
  {

    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "build/index.html"
          },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
     
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }